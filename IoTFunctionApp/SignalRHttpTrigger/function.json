{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "signalRConnectionInfo",
      "name": "connectionInfoJson",
      "hubName": "%AzureSignalRHubName%",
      "connectionStringSetting": "AzureSignalRConnectionString",
      "direction": "in"
  },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
